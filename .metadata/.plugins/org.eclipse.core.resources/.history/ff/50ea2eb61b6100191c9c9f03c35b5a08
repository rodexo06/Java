package enums;

public enum Comodos {
	
	Cozinha(1), Sala(2), Quartos(3), Banheiro(4), Quintal(5), AreaServico(6);  

	private int valor;
	
	private Comodos(int valor) {
		this.valor = valor;
	}
	
	public int getValor() {
		return valor;
	}
}

public enum Cozinhad {
	
	
	Lampadas(1), Fogao(2), Microondas(3),Torneira(4), Geladeira(5),	Cafeteira(6), Purifcador(7);

	private int valor;
	
	private Cozinhad(int valor) {
		this.valor = valor;
	}
	
	public int getValor() {
		return valor;
	}
}