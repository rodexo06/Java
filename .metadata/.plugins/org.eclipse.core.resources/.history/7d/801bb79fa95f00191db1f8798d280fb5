
import javax.swing.JOptionPane;

public class Fogao extends Cozinha {
	
	
	
	public Fogao() {
		super();
		this.quantidade++;
		setQuantidade(quantidade);
		definirVoltagem();
	}
	
	public void calculoGasto() {
		consumoobj = getQuantidade() * getConsumo();
		this.setConsumoobj(consumoobj);
	}

	public void mostrarObj() {
		JOptionPane.showMessageDialog(null, "Fogão"+"\t"+"Voltagem: " + getVoltagem() +  "\n" + "Quantidade: " + getQuantidade() +  "\n"
											+ "Consumo do produto: " + getConsumoobj() );
	}


}
