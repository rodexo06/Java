								geladeira.mostrarObj(geladeira);
								opc = JOptionPane.showInputDialog(tx.getTextOpc());
								return;
							}
							if (menu4 == 1) {
								geladeira.menosUm();
								geladeira.calculoGasto();
								geladeira.mostrarObj(geladeira);
								opc = JOptionPane.showInputDialog(tx.getTextOpc());
								return;
							}
							if (menu4 == 1) {
								geladeira.calcularHora();
								geladeira.calculoGasto();
								geladeira.mostrarObj(geladeira);
								opc = JOptionPane.showInputDialog(tx.getTextOpc());
								return;
							}
